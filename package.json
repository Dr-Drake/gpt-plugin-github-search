{
  "name": "gpt-plugin-github-search",
  "version": "1.0.0",
  "description": "An express application that exposes endpoints specifically designed for chatgpt to call when prompted about github repositories.",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && npx tsc && cpy \"src/public/**/*\" \"dist/src/public\"",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "start": "node dist/src/index.js",
    "watch": "nodemon"
  },
  "keywords": [
    "chatgpt",
    "plugin",
    "express",
    "nodejs",
    "api"
  ],
  "author": "Ikem Ezechukwu",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.7",
    "concurrently": "^8.0.1",
    "cpy-cli": "^4.2.0",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ]
}
